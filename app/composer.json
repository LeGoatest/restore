{
    "name": "my-restore-pro/website",
    "description": "MyRestorePro - Service Website",
    "type": "project",
    "require": {
        "php": ">=8.1",
        "ext-pdo": "*",
        "ext-pdo_sqlite": "*",
        "brick/date-time": "^0.7.0",
        "casbin/casbin": "^4.0",
        "doctrine/orm": "^3.5",
        "egulias/email-validator": "^4.0",
        "guzzlehttp/guzzle": "^7.10",
        "intervention/image": "^3.11",
        "league/commonmark": "^2.7",
        "league/flysystem": "^3.30",
        "nesbot/carbon": "^3.10",
        "phpmailer/phpmailer": "^6.10",
        "propaganistas/laravel-phone": "^6.0",
        "spatie/async": "^1.6",
        "twig/twig": "^3.21",
        "zircote/swagger-php": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "require-dev": {
        "fakerphp/faker": "^1.24",
        "larapack/dd": "^1.1",
        "nelmio/alice": "^3.14",
        "pestphp/pest": "*"
    }
}
